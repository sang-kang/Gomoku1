{
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": false,
    "isolatedModules": true,
    "outDir": "dist",
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "skipLibCheck": false,
    "lib": [
        "DOM",
        "DOM.Iterable",
        "es6"
    ],
    "sourceMap": true,
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": "script",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}